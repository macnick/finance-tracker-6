    <th>Company Name</th>
    <th>Price</th>
    <th>52 week low</th>
    <th>52 week high</th>
    <th>Actions</th>
  </tr></thead>
  <tbody>

      <% @my_portfolio.each do |stock| %>
        <tr>
          <td><%= stock.ticker %></td>
          <td><%= stock.name %></td>
          <td><%= stock.last_price %></td>
          <td>0</td>
          <td>0</td>
          <td>--</td>
        </tr>

      <% end %>
  </tbody>
</table>